<?xml version="1.0" encoding="utf-8" ?><nlog xmlns="http://www.nlog-project.org/schemas/NLog.xsd"      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"      xsi:schemaLocation="http://www.nlog-project.org/schemas/NLog.xsd NLog.xsd"      autoReload="true"      throwExceptions="false"      internalLogLevel="Info" 	  internalLogFile="d:\\HardcoreFramework\\HardcoreFramework\\Target\\Logs\\console-example-internal.log"><targets>    <!-- write logs to the files -->    <target xsi:type="File" name="all_logs_file" fileName="d:\\HardcoreFramework\\HardcoreFramework\\Target\\Logs\\all.log"/>    <target xsi:type="File" name="important_logs_file" fileName="d:\\HardcoreFramework\\HardcoreFramework\\Target\\Logs\\important.log"/>    <!-- write logs to the console-->    <target xsi:type="Console" name="logconsole" />  </targets><rules>    <logger name="*" minlevel="Trace" writeTo="logconsole" />    <logger name="*" minlevel="Debug" writeTo="all_logs_file" />    <logger name="*" minlevel="Warn" writeTo="important_logs_file" /></rules></nlog>